 cmd_strip     = $(STRIPTOOL) $(STRIP_FLAGS) $^
 cmd_t_strip   = $(STRIPTOOL) $(STRIP_FLAGS) $@
-cmd_ar        = $(AR) $(ARFLAGS) $@ $(call objects_with_syms,,$^)
+define cmd_ar
+	$(file >$@.in,$(call objects_with_syms,,$^))
+	$(AR) $(ARFLAGS) $@ @$@.in
+	@rm $@.in
+endef
 
